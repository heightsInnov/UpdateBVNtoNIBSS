        <link href="css/theme.bootstrap.css" rel="stylesheet">
        <style>

            /* Sticky footer styles
            -------------------------------------------------- */
            html {
                position: relative;
                min-height: 100%;
            }
            body {
                /* Margin bottom by footer height */
                margin-bottom: 70px;
            }
            #footer {
                position: absolute;
                bottom: 0;
                width: 100%;
                /* Set the fixed height of the footer here */
                height: 70px;
                background-color: #f5f5f5;
                background-image: url(/pitch/img/404-footer.jpg)
            }


            /* Custom page CSS
            -------------------------------------------------- */
            /* Not required for template or sticky footer method. */

            body > .container {
                padding: 60px 15px 0;
            }
            .container .text-muted {
                margin: 30px 0 0;
                color: white;
            }

            #footer > .container {
                padding-right: 15px;
                padding-left: 15px;
            }

            code {
                font-size: 80%;
            }
        </style>
    </head>
    <body> 

        <!-- Fixed navbar -->
        <div id="banner" class="navbar navbar-inverse navbar-static-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="./">UBN Payment Portal</a>
                </div>
                <div class="collapse navbar-collapse">                  
                    <ul class="nav navbar-nav navbar-right">
                        <li><p class="navbar-text"><strong>Union Bank of Nigeria Plc</strong></p></li> 
                    </ul>
                </div><!--/.nav-collapse -->        
            </div>
        </div>

        <!-- Begin page content -->
        <div align="center">
            <div class="container">
                <div class="jumbotron" style="padding: 0;margin: 0; background-color:transparent">
                <h1><small>Sorry, Did you get lost on UBN Payment Portal? </small></h1>
                <br>
                <img src="/pitch/img/403-1.png" class="img-responsive" alt="403 ERROR">
                <br>
                <h2>We Can't Find The Page You Requested.</h2>
                <h2><small>If you are lost please contact our support team or visit our <a href="home.jsp">Homepage</a>.</small></h2>
                </div>
            </div>
            <div id="footer">
                <div class="container">
                    <p class="text-muted"><strong>&copy; Union 2014</strong></p>
                </div>
            </div>
        </div>
        <!-- Placed at the end of the document so the pages load faster -->
        <script src="js/jquery-ui-1.10.4.custom.min.js"></script>
